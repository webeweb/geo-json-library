{
    "name": "webeweb/geo-json-library",
    "description": "Integrate Geo JSON with your projects",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "webeweb",
        "library",
        "geo",
        "json"
    ],
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "homepage": "https://github.com/webeweb/geo-json-library",
    "support": {
        "issues": "https://github.com/webeweb/geo-json-library/issues"
    },
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "webeweb/core-library": "^8.0"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^7.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Library\\GeoJson\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Library\\GeoJson\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
